		},
		"maxrootdirent": {
			"type": "uint_t",
			"title": "Rm",
			"description": "Maxmimum root directories allowed given root directory size"
		},
		"totalsect": {
			"type": "uint_t",
			"title": "TS",
			"description": "Total sectors"
		},
		"usablesect": {
			"type": "uint_t",
			"title": "US",
			"description": "Usable sectors"
		},
		"usablecluster": {
			"type": "uint_t",
			"title": "UC",
			"description": "Usable clusters"
		},
		"mindosver": {
			"type": "string",
			"title": "DOS",
			"description": "Minimum DOS version"
		},
		"comusesys": {
			"type": "string",
			"title": "System",
			"description": "Systems that commonly use this format"
		}
	}
