{
	"table": "Floppy Disk Format Summary",
	"id": "msdos-floppy-disk-format-summary",
	"combinable": true,
	"notes": [
		"A summary of MS-DOS disk formats",
		"Despite the title, borrowed from the PC Programmer's Sourcebook, this table has been rearranged to provide more general information",
		"Filesystem details, like reserved sectors, sectors/FAT, root directory sectors, etc. can vary according to the BIOS Parameter Block. They are not fixed, except for older DOS versions"
	],
	"base definition": true,
	"table format": {
		"order": [ "key", "disk", "midb", "heads", "tracks", "sectors", "bytespsector", "sectpcluster", "reservedsect", "sectpfat", "nfattabl", "rootdirent", "maxrootdirent", "totalsect", "usablesect", "usablecluster", "mindosver", "comusesys" ]
	},
	"table columns": {
		"key": {
			"type": "string",
			"column": "format",
			"title": "Format",
			"json": "key",
			"description": "Disk format"
		},
		"disk": {
			"type": "string",
			"case insensitive": true,
			"combine different": true,
			"title": "Disk",
			"description": "Disk size, 3.5 inch or 5.25 in, etc"
		},
		"midb": {
			"type": "uint_t",
			"title": "ID",
			"description": "Media ID byte"
		},
		"heads": {
			"type": "uint_t",
			"title": "H",
			"description": "Heads, or more appropriately, sides"
		},
		"tracks": {
			"type": "uint_t",
			"title": "T",
			"description": "Tracks per side"
		},
		"sectors": {
			"type": "uint_t",
			"title": "S",
			"description": "Sectors per track"
		},
		"bytespsector": {
			"type": "uint_t",
			"title": "B/S",
			"description": "Bytes per sector"
		},
		"sectpcluster": {
			"type": "uint_t",
			"title": "S/C",
			"description": "Sectors per cluster"
		},
		"reservedsect": {
			"type": "uint_t",
			"title": "Res",
			"description": "Reserved sector count"
		},
		"sectpfat": {
			"type": "uint_t",
			"title": "SF",
			"description": "Sectors per FAT"
		},
		"nfattabl": {
			"type": "uint_t",
			"title": "FT",
