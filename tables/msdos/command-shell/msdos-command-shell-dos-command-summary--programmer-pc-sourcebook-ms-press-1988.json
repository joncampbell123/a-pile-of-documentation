{
	"id": "msdos-command-shell-dos-command-summary",
	"source": {
		"book": "programmer-pc-sourcebook-ms-press-1988",
		"subsection": "2.01"
	},
	"table": {
		"APPEND": { "value": [ "d:path[,[d:]path...]", "Locates files outside the current directory", "APPEND c:\\dbfiles", "" ] },
		"ASSIGN": { "value": [ "[x[=] y [...]]", "Routes disk I/O to another drive", "ASSIGN a=c b=c", "" ] },
		"ATTRIB": { "value": [ "[(+|-)parms] [d:][path]filespec[/parms]", "Modifies file attributes", "ATTRIB -r myfile.doc /s", "" ] },
		"BACKUP": { "value": [ "d:[path][filespec] d:[/parms]", "Backs up one disk to another", "BACKUP c:*.* a: /S/F", "" ] },
		"BREAK": { "value": [ "[ON|OFF]", "Defines status of control break", "BREAK ON", "" ] },
		"CHCP": { "value": [ "[number]", "Selects code page DOS uses", "CHCP 850", "Must execute NLSFUNC first" ] },
		"CHDIR": { "value": [ "[d:][path]", "Sets or displays current path", "CD c:\\wp\\word\\data", "" ] },
		"CD": { "value": [ "[d:][path]", "Sets or displays current path", "CD c:\\wp\\word\\data", "" ] },
		"CHKDSK": { "value": [ "[d:][path][filespec][/parms]", "Analyzes disk and FAT", "CHKDSK c:/F", "" ] },
		"CLS": { "value": [ "", "Clears display screen", "CLS", "" ] },
		"COMMAND": { "value": [ "[d:][path][/parms", "Starts a secondary command processor", "COMMAND c:\\ut /p", "" ] },
		"COMP": { "value": [ "[d:][path][filespec] [d:][path][filespec]", "Compares contents of files", "COMP *.BAT A*.BAT", "" ] },
		"COPY": { "value": [ "[/parms] [d:][path]filespec[/parms]\n[d:][path][filespec][/parms]", "Copies a file or set of files", "COPY *.* c:", "" ] },
		"CTTY": { "value": [ "devicename", "Changes stdin and stdout to auxiliary console", "CTTY COM1", "" ] },
		"DATE": { "value": [ "[date]", "Enters, changes, or displays date", "DATE 1-12-87", "" ] },
		"DEL": { "value": [ "[d:][path]filespec", "Deletes specified file or files", "DEL *.bak", "" ] },
		"DIR": { "value": [ "[d:][path][filespec][/parms]", "Lists directory entries", "DIR c:/w", "" ] },
		"DISKCOMP": { "value": [ "[d: [d:]][/parms]", "Compares contents of two disks", "DISKCOMP a: b:", "" ] },
		"DISKCOPY": { "value": [ "[d: [d:]][/parms]", "Copies a disk", "DISKCOPY a: b:", "" ] },
		"ERASE": { "value": [ "[d:][path]filespec", "Deletes specified file or files", "ERASE *.bak", "" ] }
	}
}
