	},
	"table columns": [ "command", "syntax", "function", "example", "notes" ],
	"table": [
		[ "APPEND", "d:path[,[d:]path...]", "Locates files outside the current directory", "APPEND c:\\dbfiles", "" ],
		[ "ASSIGN", "[x[=] y [...]]", "Routes disk I/O to another drive", "ASSIGN a=c b=c", "" ],
		[ "ATTRIB", "[(+|-)parms] [d:][path]filespec[/parms]", "Modifies file attributes", "ATTRIB -r myfile.doc /s", "" ],
		[ "BACKUP", "d:[path][filespec] d:[/parms]", "Backs up one disk to another", "BACKUP c:*.* a: /S/F", "" ],
		[ "BREAK", "[ON|OFF]", "Defines status of control break", "BREAK ON", "" ],
		[ "CHCP", "[number]", "Selects code page DOS uses", "CHCP 850", "Must execute NLSFUNC first" ],
		[ "CHDIR", "[d:][path]", "Sets or displays current path", "CD c:\\wp\\word\\data", "" ],
		[ "CD", "[d:][path]", "Sets or displays current path", "CD c:\\wp\\word\\data", "" ],
		[ "CHKDSK", "[d:][path][filespec][/parms]", "Analyzes disk and FAT", "CHKDSK c:/F", "" ],
		[ "CLS", "", "Clears display screen", "CLS", "" ],
		[ "COMMAND", "[d:][path][/parms", "Starts a secondary command processor", "COMMAND c:\\ut /p", "" ],
		[ "COMP", "[d:][path][filespec] [d:][path][filespec]", "Compares contents of files", "COMP *.BAT A*.BAT", "" ],
		[ "COPY", "[/parms] [d:][path]filespec[/parms]\n[d:][path][filespec][/parms]", "Copies a file or set of files", "COPY *.* c:", "" ],
		[ "CTTY", "devicename", "Changes stdin and stdout to auxiliary console", "CTTY COM1", "" ],
		[ "DATE", "[date]", "Enters, changes, or displays date", "DATE 1-12-87", "" ],
		[ "DEL", "[d:][path]filespec", "Deletes specified file or files", "DEL *.bak", "" ],
		[ "DIR", "[d:][path][filespec][/parms]", "Lists directory entries", "DIR c:/w", "" ],
		[ "DISKCOMP", "[d: [d:]][/parms]", "Compares contents of two disks", "DISKCOMP a: b:", "" ],
		[ "DISKCOPY", "[d: [d:]][/parms]", "Copies a disk", "DISKCOPY a: b:", "" ],
		[ "ERASE", "[d:][path]filespec", "Deletes specified file or files", "ERASE *.bak", "" ],
		[ "FASTOPEN", "d:[=number]", "Stores location of opened files in memory", "FASTOPEN c:=100", "" ],
		[ "FDISK", "", "Creates, changes, or displays DOS partitions", "FDISK", "" ],
		[ "FIND", "[/parms] \"string\" [[d:][path]filespec]", "Finds lines in files with matching \"string\"", "FIND /N \"Thom\" c:m./file.dat", "" ],
		[ "FORMAT", "d:[/parms]", "Formats disk for use", "FORMAT a:/s", "" ],
		[ "GRAFTABL", "[437|850|863|865|STATUS]", "Loads additional character data into memory", "GRAFTABL 437", "" ],
		[ "GRAPHICS", "[printer][/parms]", "Sets system to print graphic displays", "GRAPHICS COLOR1 /R", "" ],
		[ "JOIN", "JOIN", "Shows logically connected drives", "JOIN", "" ],
		[ "JOIN", "JOIN d: d:\\directory", "Logically connects drives", "JOIN b: c:", "" ],
		[ "JOIN", "JOIN d: /D", "Disconnects logically connected drives", "JOIN b: /d", "" ],
		[ "KEYB", "KEYB[xx,[yyy],[[d:][path]filespec]]", "Loads keyboard driver to replace BIOS routines", "KEYBUK", "[xx] is a 2-byte suffix indicating the country i.e. UK" ],
		[ "LABEL", "[d:][label]", "Creates, changes, or displays volume label", "LABEL c:thomhogan", "" ],
		[ "MKDIR", "[d:][path]", "Creates subdirectory", "MKDIR c:\\ss\\excel\\data", "May be abbreviated MD" ],
		[ "MODE", "LPT#[:][n][,m][,p]", "Sets printer specifications", "MODE lpt1:132,8,p", "" ],
		[ "MODE", "n", "Sets video display mode", "MODE CO80", "" ],
		[ "MODE", "[n],m[,T]", "Sets video display mode", "MODE 80,r,t", "" ],
		[ "MODE", "COM#[:]baud[,[parity][,[databits][,[stopbits][,P]]]]", "Sets serial port specifications", "MODE com1:12,n,8,1,p", "" ],
		[ "MODE", "LPT#[:]=COM#", "Redirects parallel printer output", "MODE lpt1:=com1", "" ],
		[ "MODE", "device CODEPAGE PREPARE=((cp) [d:][path][filespec]", "Prepares code pages", "MODE lpt1 cp prep=((850,863) 4201.cpi)", "" ],
		[ "MODE", "device CODEPAGE PREPARE=((cplist) [d:][path][filespec]", "Prepares code pages", "MODE lpt1 cp prep=((850) 4201.cpi)", "" ],
		[ "MODE", "device CODEPAGE SELECT=cp", "Selects or activates code pages", "MODE lpt1 cp select=863", "" ],
		[ "MODE", "device CODEPAGE [/STATUS]", "Displays active code page", "MODE lpt1 cp", "" ],
		[ "MODE", "device CODEPAGE REFRESH", "Refreshes a code page", "MODE lpt1 cp refresh", "" ],
		[ "MORE", "", "Reads data for stdin, sends pages to stdout", "MORE <screen.doc", "" ],
		[ "NLSFUNC", "[d:][path][filespec]", "Provides extended country support", "NLSFUNC c:\\uf\\country.sys", "" ],
		[ "PATH", "[[d:][path][[;[d:][path]]]]", "Sets search path for commands", "PATH c:'c:\\ut;c:\\dos", "" ],
		[ "PRINT", "[/parms][d:][path][filespec]...]", "Puts selected file in print queue", "PRINT c:*.asm", "" ],
		[ "PROMPT", "[prompt]", "Sets new DOS prompt", "PROMPT $p$g", "" ],
		[ "RECOVER", "[d:][path]filespec", "Recovers files from defective disk", "RECOVER c:", "" ],
		[ "RECOVER", "d:", "Recovers files from defective disk", "RECOVER c:", "" ],
		[ "RENAME", "[d:][path]filespec1 filespec2", "Renames a file", "RENAME b:tom thom", "" ],
		[ "REPLACE", "[d:][path]filespec [d:][path][/parms]", "Selectively replaces matching files on target", "REPLACE a:*.doc c:\\ /s/p", "" ],
		[ "RESTORE", "d: [d:][path]filespec[/parms]", "Restores backed up files to disk", "RESTORE a: c:\\*.* /s", "" ],
		[ "RMDIR", "[d:]path", "Deletes a subdirectory from disk", "RMDIR c:\\lotus\\123", "" ],
		[ "RD", "[d:]path", "Deletes a subdirectory from disk", "RD c:\\lotus\\123", "" ],
		[ "SELECT", "[[A:][B:][d:][path]] xxx yy", "Installs DOS on new disk", "SELECT a: 001", "" ],
		[ "SET", "[name=[value]]", "Installs string with value in environment", "SET sidekickin = YES", "" ],
		[ "SHARE", "[/parms]", "Loads file sharing support", "SHARE /F:4048", "" ],
		[ "SORT", "[/parms", "Sorts data read from stdin, sends to stdout", "dir | SORT /+14", "" ],
		[ "SUBST", "d: d:path", "Sets up different drive specifier for drive or path", "SUBST a: c:\\wp\\ws", "" ],
		[ "SUBST", "d: /D", "Deletes SUBSTed drive", "SUBST a: /D", "" ],
		[ "SUBST", "", "Shows SUBSTed drives", "SUBST", "" ],
		[ "SYS", "d:", "Copies DOS onto disk", "SYS a:", "" ],
		[ "TIME", "[/time", "Sets, changes, or displays time", "TIME 15:10", "" ],
		[ "TREE", "[d:][/F]", "Displays directory paths", "TREE c:/f", "" ]
	]
}
