{
	"id": "msdos-command-shell-dos-command-summary",
	"source": {
		"book": "programmer-pc-sourcebook-ms-press-1988",
		"subsection": "2.01"
	},
	"table": {
		"APPEND": { "value": [ "d:path[,[d:]path...]", "Locates files outside the current directory", "APPEND c:\\dbfiles", "" ] },
		"ASSIGN": { "value": [ "[x[=] y [...]]", "Routes disk I/O to another drive", "ASSIGN a=c b=c", "" ] },
		"ATTRIB": { "value": [ "[(+|-)parms] [d:][path]filespec[/parms]", "Modifies file attributes", "ATTRIB -r myfile.doc /s", "" ] },
		"BACKUP": { "value": [ "d:[path][filespec] d:[/parms]", "Backs up one disk to another", "BACKUP c:*.* a: /S/F", "" ] },
		"BREAK": { "value": [ "[ON|OFF]", "Defines status of control break", "BREAK ON", "" ] },
		"CHCP": { "value": [ "[number]", "Selects code page DOS uses", "CHCP 850", "Must execute NLSFUNC first" ] },
		"CHDIR": { "value": [ "[d:][path]", "Sets or displays current path", "CD c:\\wp\\word\\data", "" ] },
		"CD": { "value": [ "[d:][path]", "Sets or displays current path", "CD c:\\wp\\word\\data", "" ] },
		"CHKDSK": { "value": [ "[d:][path][filespec][/parms]", "Analyzes disk and FAT", "CHKDSK c:/F", "" ] },
		"CLS": { "value": [ "", "Clears display screen", "CLS", "" ] },
		"COMMAND": { "value": [ "[d:][path][/parms", "Starts a secondary command processor", "COMMAND c:\\ut /p", "" ] },
		"COMP": { "value": [ "[d:][path][filespec] [d:][path][filespec]", "Compares contents of files", "COMP *.BAT A*.BAT", "" ] },
		"COPY": { "value": [ "[/parms] [d:][path]filespec[/parms]\n[d:][path][filespec][/parms]", "Copies a file or set of files", "COPY *.* c:", "" ] },
		"CTTY": { "value": [ "devicename", "Changes stdin and stdout to auxiliary console", "CTTY COM1", "" ] },
		"DATE": { "value": [ "[date]", "Enters, changes, or displays date", "DATE 1-12-87", "" ] },
		"DEL": { "value": [ "[d:][path]filespec", "Deletes specified file or files", "DEL *.bak", "" ] },
		"DIR": { "value": [ "[d:][path][filespec][/parms]", "Lists directory entries", "DIR c:/w", "" ] },
		"DISKCOMP": { "value": [ "[d: [d:]][/parms]", "Compares contents of two disks", "DISKCOMP a: b:", "" ] },
		"DISKCOPY": { "value": [ "[d: [d:]][/parms]", "Copies a disk", "DISKCOPY a: b:", "" ] },
		"ERASE": { "value": [ "[d:][path]filespec", "Deletes specified file or files", "ERASE *.bak", "" ] },
		"FASTOPEN": { "value": [ "d:[=number]", "Stores location of opened files in memory", "FASTOPEN c:=100", "" ] },
		"FDISK": { "value": [ "", "Creates, changes, or displays DOS partitions", "FDISK", "" ] },
		"FIND": { "value": [ "[/parms] \"string\" [[d:][path]filespec]", "Finds lines in files with matching \"string\"", "FIND /N \"Thom\" c:m./file.dat", "" ] },
		"FORMAT": { "value": [ "d:[/parms]", "Formats disk for use", "FORMAT a:/s", "" ] },
		"GRAFTABL": { "value": [ "[437|850|863|865|STATUS]", "Loads additional character data into memory", "GRAFTABL 437", "" ] },
		"GRAPHICS": { "value": [ "[printer][/parms]", "Sets system to print graphic displays", "GRAPHICS COLOR1 /R", "" ] },
		"JOIN": [
			{ "value": [ "JOIN", "Shows logically connected drives", "JOIN", "" ] },
			{ "value": [ "JOIN d: d:\\directory", "Logically connects drives", "JOIN b: c:", "" ] },
			{ "value": [ "JOIN d: /D", "Disconnects logically connected drives", "JOIN b: /d", "" ] }
		],
		"KEYB": { "value": [ "KEYB[xx,[yyy],[[d:][path]filespec]]", "Loads keyboard driver to replace BIOS routines", "KEYBUK", "[xx] is a 2-byte suffix indicating the country i.e. UK" ] },
		"LABEL": { "value": [ "[d:][label]", "Creates, changes, or displays volume label", "LABEL c:thomhogan", "" ] },
		"MKDIR": { "value": [ "[d:][path]", "Creates subdirectory", "MKDIR c:\\ss\\excel\\data", "May be abbreviated MD" ] },
		"MODE": [
			{ "value": [ "LPT#[:][n][,m][,p]", "Sets printer specifications", "MODE lpt1:132,8,p", "" ] },
			{ "value": [ "n", "Sets video display mode", "MODE CO80", "" ] },
			{ "value": [ "[n],m[,T]", "Sets video display mode", "MODE 80,r,t", "" ] },
			{ "value": [ "COM#[:]baud[,[parity][,[databits][,[stopbits][,P]]]]", "Sets serial port specifications", "MODE com1:12,n,8,1,p", "" ] },
			{ "value": [ "LPT#[:]=COM#", "Redirects parallel printer output", "MODE lpt1:=com1", "" ] },
			{ "value": [ "device CODEPAGE PREPARE=((cp) [d:][path][filespec]", "Prepares code pages", "MODE lpt1 cp prep=((850,863) 4201.cpi)", "" ] },
			{ "value": [ "device CODEPAGE PREPARE=((cplist) [d:][path][filespec]", "Prepares code pages", "MODE lpt1 cp prep=((850) 4201.cpi)", "" ] },
			{ "value": [ "device CODEPAGE SELECT=cp", "Selects or activates code pages", "MODE lpt1 cp select=863", "" ] },
			{ "value": [ "device CODEPAGE [/STATUS]", "Displays active code page", "MODE lpt1 cp", "" ] },
			{ "value": [ "device CODEPAGE REFRESH", "Refreshes a code page", "MODE lpt1 cp refresh", "" ] }
		],
		"MORE": { "value": [ "", "Reads data for stdin, sends pages to stdout", "MORE <screen.doc", "" ] },
		"NLSFUNC": { "value": [ "[d:][path][filespec]", "Provides extended country support", "NLSFUNC c:\\uf\\country.sys", "" ] },
		"PATH": { "value": [ "[[d:][path][[;[d:][path]]]]", "Sets search path for commands", "PATH c:'c:\\ut;c:\\dos", "" ] },
		"PRINT": { "value": [ "[/parms][d:][path][filespec]...]", "Puts selected file in print queue", "PRINT c:*.asm", "" ] },
		"PROMPT": { "value": [ "[prompt]", "Sets new DOS prompt", "PROMPT $p$g", "" ] },
		"RECOVER": [
			{ "value": [ "[d:][path]filespec", "Recovers files from defective disk", "RECOVER c:", "" ] },
			{ "value": [ "d:", "Recovers files from defective disk", "RECOVER c:", "" ] }
		],
		"RENAME": { "value": [ "[d:][path]filespec1 filespec2", "Renames a file", "RENAME b:tom thom", "" ] },
		"REPLACE": { "value": [ "[d:][path]filespec [d:][path][/parms]", "Selectively replaces matching files on target", "REPLACE a:*.doc c:\\ /s/p", "" ] },
		"RESTORE": { "value": [ "d: [d:][path]filespec[/parms]", "Restores backed up files to disk", "RESTORE a: c:\\*.* /s", "" ] },
		"RMDIR": { "value": [ "[d:]path", "Deletes a subdirectory from disk", "RMDIR c:\\lotus\\123", "" ] },
		"RD": { "value": [ "[d:]path", "Deletes a subdirectory from disk", "RD c:\\lotus\\123", "" ] }
	}
}
